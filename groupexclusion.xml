<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Group Test Suite">
<test name="Group StringLengthSort Tests">
    <groups>
        <run>
            <include name="sortingLength"/>
        </run>
    </groups>
    <classes>
        <class name="com.beginsecure.domain.MainTest"/>
    </classes>
</test>
<test name="Group parametrized Tests">
    <groups>
        <run>
            <exclude name="sortingLength"/>
            <include name="parametrized"/>
        </run>
    </groups>
    <classes>
        <class name="com.beginsecure.domain.MainTest"/>
    </classes>
</test>
</suite>